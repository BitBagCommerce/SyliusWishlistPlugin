<?xml version="1.0" encoding="UTF-8" ?>
<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services
               http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <service id="bitbag_sylius_wishlist_plugin.action.api_platform.remove_product_from_wishlist_action" class="BitBag\SyliusWishlistPlugin\Action\ApiPlatform\RemoveProductFromWishlistAction">
            <argument type="service" id="api_platform.message_bus"/>
            <tag name="controller.service_arguments"/>
        </service>

        <service id="bitbag_sylius_wishlist_plugin.action.api_platform.remove_product_variant_from_wishlist_action" class="BitBag\SyliusWishlistPlugin\Action\ApiPlatform\RemoveProductVariantFromWishlistAction">
            <argument type="service" id="api_platform.message_bus"/>
            <tag name="controller.service_arguments"/>
        </service>

        <service id="bitbag_sylius_wishlist_plugin.action.api_platform.remove_wishlist_action" class="BitBag\SyliusWishlistPlugin\Action\ApiPlatform\RemoveWishlistAction">
            <argument type="service" id="api_platform.message_bus"/>
            <tag name="controller.service_arguments"/>
        </service>

        <service id="bitbag_sylius_wishlist_plugin.action.base_wishlist_products_action" abstract="true" class="BitBag\SyliusWishlistPlugin\Action\BaseWishlistProductsAction">
            <argument type="service" id="sylius.context.cart.new"/>
            <argument type="service" id="form.factory"/>
            <argument type="service" id="request_stack"/>
            <argument type="service" id="bitbag_sylius_wishlist_plugin.processor.wishlist_command_processor"/>
            <argument type="service" id="sylius.command_bus"/>
            <argument type="service" id="router"/>
            <argument type="service" id="bitbag_sylius_wishlist_plugin.repository.wishlist"/>
            <argument type="service" id="translator"/>
        </service>

        <service id="bitbag_sylius_wishlist_plugin.action.base_wishlists_listing_action" abstract="true" class="BitBag\SyliusWishlistPlugin\Action\BaseWishlistsListingAction">
            <argument type="service" id="twig"/>
            <argument type="service" id="bitbag_sylius_wishlist_plugin.resolver.wishlists_resolver"/>
        </service>

        <service id="bitbag_sylius_wishlist_plugin.action.list_wishlists" class="BitBag\SyliusWishlistPlugin\Action\ListWishlistsAction" parent="bitbag_sylius_wishlist_plugin.action.base_wishlists_listing_action">
            <tag name="controller.service_arguments"/>
        </service>

        <service id="bitbag_sylius_wishlist_plugin.action.render_header_template" class="BitBag\SyliusWishlistPlugin\Action\RenderHeaderTemplateAction" parent="bitbag_sylius_wishlist_plugin.action.base_wishlists_listing_action">
            <tag name="controller.service_arguments"/>
        </service>

        <service id="bitbag_sylius_wishlist_plugin.action.add_product_to_wishlist" class="BitBag\SyliusWishlistPlugin\Action\AddProductToWishlistAction">
            <argument type="service" id="sylius.repository.product"/>
            <argument type="service" id="bitbag_sylius_wishlist_plugin.factory.wishlist_product"/>
            <argument type="service" id="request_stack"/>
            <argument type="service" id="translator"/>
            <argument type="service" id="bitbag_sylius_wishlist_plugin.resolver.wishlists_resolver"/>
            <argument type="service" id="bitbag_sylius_wishlist_plugin.manager.wishlist"/>
            <argument type="service" id="sylius.context.channel"/>
            <tag name="controller.service_arguments"/>
        </service>

        <service id="bitbag_sylius_wishlist_plugin.action.remove_product_from_wishlist" class="BitBag\SyliusWishlistPlugin\Action\RemoveProductFromWishlistAction">
            <argument type="service" id="bitbag_sylius_wishlist_plugin.context.wishlist"/>
            <argument type="service" id="sylius.repository.product"/>
            <argument type="service" id="bitbag_sylius_wishlist_plugin.manager.wishlist_product"/>
            <argument type="service" id="request_stack"/>
            <argument type="service" id="translator"/>
            <argument type="service" id="router"/>
            <tag name="controller.service_arguments"/>
        </service>

        <service id="bitbag_sylius_wishlist_plugin.action.clean_wishlist" class="BitBag\SyliusWishlistPlugin\Action\CleanWishlistAction">
            <argument type="service" id="bitbag_sylius_wishlist_plugin.repository.wishlist"/>
            <argument type="service" id="bitbag_sylius_wishlist_plugin.manager.wishlist_product"/>
            <argument type="service" id="request_stack"/>
            <argument type="service" id="translator"/>
            <argument type="service" id="router"/>
            <tag name="controller.service_arguments"/>
        </service>

        <service id="bitbag_sylius_wishlist_plugin.action.add_product_variant_to_wishlist" class="BitBag\SyliusWishlistPlugin\Action\AddProductVariantToWishlistAction">
            <argument type="service" id="sylius.repository.product_variant"/>
            <argument type="service" id="bitbag_sylius_wishlist_plugin.factory.wishlist_product"/>
            <argument type="service" id="request_stack"/>
            <argument type="service" id="translator"/>
            <argument type="service" id="router"/>
            <argument type="service" id="bitbag_sylius_wishlist_plugin.repository.wishlist"/>
            <tag name="controller.service_arguments"/>
        </service>

        <service id="bitbag_sylius_wishlist_plugin.action.remove_product_variant_from_wishlist" class="BitBag\SyliusWishlistPlugin\Action\RemoveProductVariantFromWishlistAction">
            <argument type="service" id="bitbag_sylius_wishlist_plugin.repository.wishlist"/>
            <argument type="service" id="sylius.repository.product_variant"/>
            <argument type="service" id="bitbag_sylius_wishlist_plugin.manager.wishlist_product"/>
            <argument type="service" id="request_stack"/>
            <argument type="service" id="translator"/>
            <argument type="service" id="router"/>
            <tag name="controller.service_arguments"/>
        </service>

        <service id="bitbag_sylius_wishlist_plugin.action.remove_selected_products_from_wishlist" class="BitBag\SyliusWishlistPlugin\Action\RemoveSelectedProductsFromWishlistAction" parent="bitbag_sylius_wishlist_plugin.action.base_wishlist_products_action" public="true">
            <tag name="controller.service_arguments"/>
        </service>

        <service id="bitbag_sylius_wishlist_plugin.action.add_selected_products_to_cart" class="BitBag\SyliusWishlistPlugin\Action\AddSelectedProductsToCartAction" parent="bitbag_sylius_wishlist_plugin.action.base_wishlist_products_action" public="true">
            <tag name="controller.service_arguments"/>
        </service>

        <service id="bitbag_sylius_wishlist_plugin.action.export_selected_products_to_csv" class="BitBag\SyliusWishlistPlugin\Action\ExportSelectedProductsToCsvAction">
            <argument type="service" id="sylius.context.cart.new_shop_based.inner"/>
            <argument type="service" id="form.factory"/>
            <argument type="service" id="request_stack"/>
            <argument type="service" id="bitbag_sylius_wishlist_plugin.processor.wishlist_command_processor"/>
            <argument type="service" id="router"/>
            <argument type="service" id="translator"/>
            <argument type="service" id="bitbag_sylius_wishlist_plugin.repository.wishlist"/>
            <argument type="service" id="sylius.command_bus"/>
            <tag name="controller.service_arguments"/>
        </service>

        <service id="bitbag_sylius_wishlist_plugin.action.import_from_csv" class="BitBag\SyliusWishlistPlugin\Action\ImportWishlistFromCsvAction">
            <argument type="service" id="form.factory"/>
            <argument type="service" id="request_stack"/>
            <argument type="service" id="twig"/>
            <argument type="service" id="bitbag_sylius_wishlist_plugin.resolver.wishlists_resolver"/>
            <argument type="service" id="sylius.command_bus"/>
            <tag name="controller.service_arguments"/>
        </service>

        <service id="bitbag_sylius_wishlist_plugin.action.list_wishlist_products" class="BitBag\SyliusWishlistPlugin\Action\ListWishlistProductsAction">
            <argument type="service" id="sylius.context.cart"/>
            <argument type="service" id="form.factory"/>
            <argument type="service" id="twig"/>
            <argument type="service" id="bitbag_sylius_wishlist_plugin.processor.wishlist_command_processor"/>
            <argument type="service" id="bitbag_sylius_wishlist_plugin.resolver.wishlists_resolver"/>
            <argument type="service" id="translator"/>
            <argument type="service" id="router"/>
            <tag name="controller.service_arguments"/>
        </service>

        <service id="bitbag_sylius_wishlist_plugin.action.add_products_to_wishlist" class="BitBag\SyliusWishlistPlugin\Action\AddProductsToCartAction" parent="bitbag_sylius_wishlist_plugin.action.base_wishlist_products_action" public="true">
            <tag name="controller.service_arguments"/>
        </service>

        <service id="bitbag_sylius_wishlist_plugin.action.export_wishlist_to_pdf_action" class="BitBag\SyliusWishlistPlugin\Action\ExportWishlistToPdfAction" parent="bitbag_sylius_wishlist_plugin.action.base_wishlist_products_action" public="true">
            <tag name="controller.service_arguments"/>
        </service>

        <service id="bitbag_sylius_wishlist_plugin.action.create_new_wishlist" class="BitBag\SyliusWishlistPlugin\Action\CreateNewWishlistAction">
            <argument type="service" id="sylius.command_bus"/>
            <argument type="service" id="request_stack"/>
            <argument type="service" id="translator"/>
            <argument type="service" id="sylius.context.channel"/>
            <argument type="service" id="router.default"/>
            <tag name="controller.service_arguments"/>
        </service>

        <service id="bitbag_sylius_wishlist_plugin.action.show_chosen_wishlist" class="BitBag\SyliusWishlistPlugin\Action\ShowChosenWishlistAction">
            <argument type="service" id="bitbag_sylius_wishlist_plugin.repository.wishlist"/>
            <argument type="service" id="sylius.context.cart"/>
            <argument type="service" id="form.factory"/>
            <argument type="service" id="twig"/>
            <argument type="service" id="bitbag_sylius_wishlist_plugin.processor.wishlist_command_processor"/>
            <argument type="service" id="router.default"/>
            <argument type="service" id="bitbag_sylius_wishlist_plugin.resolver.wishlist_cookie_token_resolver"/>
            <argument type="service" id="security.token_storage"/>
            <argument type="service" id="bitbag_sylius_wishlist_plugin.resolver.token_user_resolver"/>
            <tag name="controller.service_arguments"/>
        </service>

        <service id="bitbag_sylius_wishlist_plugin.action.add_product_to_selected_wishlist" class="BitBag\SyliusWishlistPlugin\Action\AddProductToSelectedWishlistAction">
            <argument type="service" id="bitbag_sylius_wishlist_plugin.repository.wishlist"/>
            <argument type="service" id="sylius.repository.product"/>
            <argument type="service" id="request_stack"/>
            <argument type="service" id="translator"/>
            <argument type="service" id="router"/>
            <argument type="service" id="sylius.command_bus"/>
            <tag name="controller.service_arguments"/>
        </service>

        <service id="bitbag_sylius_wishlist_plugin.action.copy_selected_products_to_other_wishlist" class="BitBag\SyliusWishlistPlugin\Action\CopySelectedProductsToOtherWishlistAction">
            <argument type="service" id="sylius.command_bus"/>
            <argument type="service" id="request_stack"/>
            <argument type="service" id="translator"/>
            <tag name="controller.service_arguments"/>
        </service>

        <service id="bitbag_sylius_wishlist_plugin.action.update_wishlist_name_action" class="BitBag\SyliusWishlistPlugin\Action\UpdateWishlistNameAction">
            <argument type="service" id="sylius.command_bus"/>
            <argument type="service" id="request_stack"/>
            <argument type="service" id="translator"/>
            <argument type="service" id="bitbag_sylius_wishlist_plugin.repository.wishlist"/>
            <argument type="service" id="router"/>
            <tag name="controller.service_arguments"/>
        </service>

        <service id="bitbag_sylius_wishlist_plugin.action.add_wishlists_to_user" class="BitBag\SyliusWishlistPlugin\Action\AddWishlistToUserAction">
            <argument type="service" id="sylius.command_bus"/>
            <argument type="service" id="request_stack"/>
            <argument type="service" id="translator"/>
            <argument type="service" id="bitbag_sylius_wishlist_plugin.repository.wishlist"/>
            <argument type="service" id="router"/>
            <argument type="service" id="security.token_storage"/>
            <argument type="service" id="bitbag_sylius_wishlist_plugin.resolver.token_user_resolver"/>
            <tag name="controller.service_arguments"/>
        </service>
    </services>
</container>
