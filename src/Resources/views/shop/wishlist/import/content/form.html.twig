{% set form = hookable_metadata.context.form %}

{{ form_start(form, { attr: { class: 'import-wishlist ui form', id: "wishlist_import_form" } }) }}
    <div class="bb-wishlist-list">
        {{ form_widget(form.wishlist_file, { attr: { class: "form-control mb-3" } } ) }}
        {{ form_widget(form.wishlists, { attr: { class: "form-select mb-3" } } ) }}
        {{ form_widget(form.submit, { attr: { class: "btn btn-primary" } } ) }}
    </div>
{{ form_end(form) }}
