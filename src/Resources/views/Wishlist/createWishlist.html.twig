{% extends '@SyliusShop/layout.html.twig' %}

{% import '@SyliusUi/Macro/messages.html.twig' as messages %}
{% form_theme form '@SyliusShop/Form/theme.html.twig' %}

{% block content %}
    {{ form_start(form) }}

    <div class="row">
        <div class="col">
            {{ form_row(form.name) }}
        </div>
        <div class="col" id="some-custom-id">
            {{ form_row(form.save) }}
        </div>
    </div>
    {{ form_end(form) }}
{% endblock %}