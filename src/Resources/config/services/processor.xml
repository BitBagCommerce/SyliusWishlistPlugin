<?xml version="1.0" encoding="UTF-8" ?>
<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services
               http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <service id="bitbag_sylius_wishlist_plugin.processor.wishlist_command_processor" class="Sylius\WishlistPlugin\Processor\WishlistCommandProcessor"/>

        <service id="bitbag_sylius_wishlist_plugin.processor.selected_wishlist_products_processor" class="Sylius\WishlistPlugin\Processor\SelectedWishlistProductsProcessor"/>

        <service id="bitbag_sylius_wishlist_plugin.processor.variant_pdf_model_processor" class="Sylius\WishlistPlugin\Processor\VariantPdfModelProcessor">
            <argument type="service" id="bitbag_sylius_wishlist_plugin.services.generator.model_creator"/>
        </service>
    </services>
</container>
